{
  "name": "library-management-system-backend",
  "version": "1.0.0",
  "description": "Library Management System Backend",
  "main": "index.js",
  "repository": "https://github.com/azad71/Library-Management-System",
  "author": "Azad Mamun",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "migrate": "sequelize-cli --options-path ./.sequelize-config db:migrate",
    "migrate:create": "sequelize-cli --options-path ./.sequelize-config migration:create --name",
    "migrate:undo": "sequelize-cli --options-path ./.sequelize-config db:migrate:undo",
    "test": "cross-env NODE_ENV=test jest --watch --verbose"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "sequelize": "^6.29.3",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "smtp-server": "^3.11.0",
    "supertest": "^6.3.3"
  }
}
